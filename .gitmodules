[submodule "shell/liquidprompt"]
	path = misc/shell/liquidprompt
	url = https://github.com/nojhan/liquidprompt
[submodule "zsh/oh-my-zsh"]
	path = zsh/.oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh
	ignore = dirty
[submodule "nvm/.nvm"]
	path = nvm/.nvm
	url = https://github.com/creationix/nvm
[submodule "misc/shell/base16-shell"]
	path = misc/shell/base16-shell
	url = https://github.com/chriskempson/base16-shell
[submodule "misc/tmux/plugins/tpm"]
	path = misc/tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "vim/.vim/bundle/Vundle.vim"]
	path = vim/.vim/bundle/Vundle.vim
	url = https://github.com/VundleVim/Vundle.vim.git
